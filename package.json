{
  "name": "mltc",
  "description": "Michael Learn To Code",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "watch:tailwind": "postcss static/tailwind.css -o static/base.css -w",
    "build:tailwind": "NODE_ENV=production postcss static/tailwind.css -o static/base.css",
    "build": "npm run build:tailwind && sapper build --legacy",
    "sapper:dev": "sapper dev",
    "dev": "run-p --race watch:tailwind sapper:dev",
    "sapper:export": "sapper export",
    "export": "npm run build:tailwind && sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@netlify/plugin-sitemap": "^0.3.3",
    "acorn": "^7.2.0",
    "bl": "^4.0.3",
    "compression": "^1.7.1",
    "date-fns": "^1.30.1",
    "global": "^4.4.0",
    "gray-matter": "^4.0.2",
    "jpeg-js": "^0.4.2",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.20",
    "marked": "^0.7.0",
    "minimist": "^1.2.5",
    "polka": "next",
    "postcss-cli": "^7.1.1",
    "prismjs": "^1.21.0",
    "reading-time": "^1.2.0",
    "serialize-javascript": "^3.1.0",
    "sirv": "^0.4.0",
    "tailwindcss": "^1.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
